<h1>Sign up</h1>
<%= form_with model: @user, url: sign_up_path do |f|  %>
    <br><%= f.label :email %>
    <br><%= f.text_field :email %>

    <br><%= f.label :password %>
    <br><%= f.password_field :password %>

    <br><%= f.label :password_confirmation %>
    <br><%= f.password_field :password_confirmation %>

    <br><br><%= f.submit "Sign Up" %>
<% end %>

<br><%= link_to "Log In", sign_in_path %>